<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table>
      <tr>
        <th>name</th>
        <th>username</th>
        <th>password</th>
        <th>email</th>
      </tr>
      <%for(var i = 0; i < employee.length; i++) {%>
      <tr>
        <td><%=employee[i].name%></td>
        <td><%=employee[i].username%></td>
        <td><%=employee[i].password%></td>
        <td><%=employee[i].email%></td>
        <td><a href="employee/edit/<%=employee[i].id%>">edit</a></td>
        <td><a href="employee/delete/<%=employee[i].id%>">delete</a></td>
      </tr>
      <%}%>
    </table>
  </body>
</html>
